{
  "Url": "http://localhost:5003",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "DBS": {
    "ConfigId": "WebApi",
    //"ConnectionString": "server=localhost;Database=testdb;Port=3306;Uid=root;Pwd=123456;Charset=utf8mb4;",
    "DbType": 1, //mysql
    "ConnectionString": "server=localhost;Database=WebApi;Port=13306;Uid=root;Pwd=Pass@word;Charset=utf8mb4;"
  },
  "JWTSettings": {
    "SecretKey": "WebApi.Core.Api@com", // 不要太短，搞复杂点
    "Issuer": "Arete",
    "Expires": 10,
    "Audience": "22333"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "restrictedToMinimumLevel": "Warning"
        }
      },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:5341"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "logs\\log-.txt",
          "rollingInterval": "Day"
        },
        "MinimumLevel": "Information"
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "Properties": {
      "Application": "MyApplication"
    }
  },
  "SnowFlake": {
    "WorkerId": 1,
    "DatacenterId": 1
  },
  "SqlAOP": {
    "Enabled": true,
    "LogToConsole": true
  },
  "SplitTable": [
    {
      "TableName": "SplitTestTable",
      "SplitType": 1
    }
  ],
  "SeedDb": {
    "SeedDBDataEnabled": true,
    "SeedDBEnabled": true
  }
}
